    <%- include('includes/head.ejs') %>
        <link rel="stylesheet" href="/css/users.css" /> 
    </head>
    <body>
        <%- include('includes/navigation.ejs') %> 
        <main>
            <h1>User List</h1>
            <% if (users.length > 0) { %>
                
            <article class="user-list">
                <% for (let user of users) { %>  
                <article class="user">
                    <p><%= user.username %> </p>
                </article>
                <% } %> 
            </article>
            <% } else { %> 
                <h1>No users not found!</h1>
            <% } %>
        </main>
    <%- include('includes/end.ejs') %> 